{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Sample": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "Debug": {
      "IncludeScopes": true
    }
  },
  "Authentication": {
    "Enabled": false,
    "ActiveDirectory": {
      "Domain": "microsoft.onmicrosoft.com",
      "TenantId": "{Azure AD Tenant ID}",
      "ClientId": "{Azure AD Client ID}",
      "ReplyUri": "https://localhost:44329/login",
      "Authority": "https://login.microsoftonline.com/{TenantId}/v2.0",
      "AccessTokenVersion": 2
    }
  }
}
